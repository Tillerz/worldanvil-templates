                  <td class='ivar od ivar-rucksack-lbs'><input value='{{attribute(variables, 'rucksack_lbs_' ~ id)|default}}' class='form-control ivar ivar-rucksack-lbs' id='rucksack_lbs_{{id}}' name='rucksack_lbs_{{id}}' type='text' /></td>
                </tr>
                {% endfor %}
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class='col-12 col-md-12'>
      <div class='card inventory' id='card-inventory'>
        <div class='card-body c'>
          Carried: {{carried}} of {{ vigor * 10 * (1 + 0.5 * (vigor // 5)) }} lbs
        </div>
      </div>
    </div>
  </div>
  <div class='row'>
    <div class='col-12 col-md-12'>
      <div class='card notes' id='card-notes'>
        <div class='card-header notes'>Notes</div>
        <div class='card-body of'>
          <div class='ivar od ivar-notes'><div class='iContent'><textarea class='form-control ivar ivar-notes mention' id='notes' name='notes' rows='4' >{{variables.notes|default}}</textarea></div></div>
        </div>
      </div>
    </div>
  </div>
</div>